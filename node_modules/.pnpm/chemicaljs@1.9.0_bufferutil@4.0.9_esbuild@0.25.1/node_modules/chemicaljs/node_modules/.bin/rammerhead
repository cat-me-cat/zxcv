#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/Metallic/node_modules/.pnpm/@rubynetwork+rammerhead@1.3.6_bufferutil@4.0.9/node_modules/@rubynetwork/rammerhead/dist/node_modules:/workspaces/Metallic/node_modules/.pnpm/@rubynetwork+rammerhead@1.3.6_bufferutil@4.0.9/node_modules/@rubynetwork/rammerhead/node_modules:/workspaces/Metallic/node_modules/.pnpm/@rubynetwork+rammerhead@1.3.6_bufferutil@4.0.9/node_modules/@rubynetwork/node_modules:/workspaces/Metallic/node_modules/.pnpm/@rubynetwork+rammerhead@1.3.6_bufferutil@4.0.9/node_modules:/workspaces/Metallic/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/Metallic/node_modules/.pnpm/@rubynetwork+rammerhead@1.3.6_bufferutil@4.0.9/node_modules/@rubynetwork/rammerhead/dist/node_modules:/workspaces/Metallic/node_modules/.pnpm/@rubynetwork+rammerhead@1.3.6_bufferutil@4.0.9/node_modules/@rubynetwork/rammerhead/node_modules:/workspaces/Metallic/node_modules/.pnpm/@rubynetwork+rammerhead@1.3.6_bufferutil@4.0.9/node_modules/@rubynetwork/node_modules:/workspaces/Metallic/node_modules/.pnpm/@rubynetwork+rammerhead@1.3.6_bufferutil@4.0.9/node_modules:/workspaces/Metallic/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@rubynetwork+rammerhead@1.3.6_bufferutil@4.0.9/node_modules/@rubynetwork/rammerhead/dist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../@rubynetwork+rammerhead@1.3.6_bufferutil@4.0.9/node_modules/@rubynetwork/rammerhead/dist/cli.js" "$@"
fi
